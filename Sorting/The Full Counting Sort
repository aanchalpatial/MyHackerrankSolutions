#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int half=n/2;
    int x;
        string s;
    vector<string> v(1000000,"");
   // cout<<"here1";
    for(int i=0;i<n;++i)
    {
        
        cin>>x>>s;
        if(i<half){
            v[x]+="- ";
        }
        else{
            v[x]+=s+" ";
        }
    }
    for(int i=0;i<n;++i)
    {
        cout<<v[i];
    }
    
   return 0;}
