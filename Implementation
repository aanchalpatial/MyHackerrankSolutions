#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n;
    cin>>n;
    for(int i=0;i<n;++i)
    {
        int grade;
        cin>>grade;
        if(grade<38)
        {
         cout<<grade<<endl;}
        else
            {
                int m=5;
                for(;m<=100;m+=5)
                    {if(m>=grade)
                    break;}
                //cout<<"m="<<m;
            if(m-grade<3)
                cout<<m<<endl;
            else
                cout<<grade<<endl;
            }
    }
    return 0;
}
